{
  "type": "project",
  "name": "viszman/rabbit",
  "keywords": [
    "PHP",
    "RabbitMQ"
  ],
  "license": "proprietary",
  "require": {
    "php": "^7.1.3",
    "php-amqplib/php-amqplib": "2.11.3",
    "symfony/yaml": "~5.0"
  },
  "require-dev": {
    "roave/security-advisories": "dev-master",
    "phpunit/phpunit": "8.4.*",
    "symfony/dotenv": "*"
  },
  "config": {
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "Viszman\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Viszman\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "auto-scripts": {
    },
    "post-install-cmd": [
      "@auto-scripts"
    ],
    "post-update-cmd": [
      "@auto-scripts"
    ]
  }
}
